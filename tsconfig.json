{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "esnext",
      "target": "ES2015",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["./src/*"],
      },
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}